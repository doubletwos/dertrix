
@model IEnumerable<Dertrix.Models.RegisteredUserOrganisation>
@if (Session["OrgId"] != null && Session["regUserOrgBrand"] != null)
{
    var currentsession = HttpContext.Current.Session.SessionID.ToString();
	var userid = Session["RegisteredUserId"];
	<ul class="nav navbar-nav navbar-right">
		<li class="dropdown">
			<a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">@Session["FullName"]<span class="caret"></span></a>
			<ul class="dropdown-menu">
				<li>
					@Html.ActionLink("Settings & Account Info", "AccountInfo", "RegisteredUsers", null,  htmlAttributes: new { title = "Settings" })
				</li>
				<li>
					@Html.ActionLink("Sign Out", "LogOut", "Access", new { id = @currentsession }, htmlAttributes: new { title = "Sign Out" })

				</li>

			</ul>
		</li>
	</ul>
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Log in", "Signin", "Access", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}
